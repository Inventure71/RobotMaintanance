{
    "version": "1.0",
    "id": "topics",
    "label": "ROS Topics",
    "icon": "ðŸ“¡",
    "steps": [
      {
        "index": 0,
        "id": "setup",
        "command": "source /opt/ros/noetic/setup.bash >/dev/null 2>&1 || true; source ~/ws/devel/setup.bash >/dev/null 2>&1 || true"
      },
      {
        "index": 1,
        "id": "list_topics",
        "command": "timeout 12s rostopic list",
        "reuseCommandOutputKey": "rostopic_list",
        "timeoutSec": 12,
        "parser": {
          "type": "topics_presence"
        }
      }
    ],
    "aggregation": "allOrNothing",
    "stopOnFailure": true,
    "manualOnly": true,
    "enabled": true,
    "defaultStatus": "warning",
    "defaultValue": "unknown",
    "defaultDetails": "Not checked yet",
    "possibleResults": [
      { "status": "ok", "value": "all_present", "details": "All required topics present" },
      { "status": "error", "value": "missing", "details": "Missing required topics" }
    ]
  }
  
